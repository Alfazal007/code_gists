{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
		"src": "dist/index.js",
		"use": "@vercel/node"
    }

  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}
